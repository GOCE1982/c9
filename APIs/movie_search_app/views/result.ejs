<h1>Results Page</h1>
<ul>
    <% results["Search"].forEach(function(movie) { %>
      <li>
          <strong>
            <%= movie["Title"] %>
          </strong>
            - <%= movie["Year"] %>
            - <%= movie["Plot"] %>
            <div style=" margin: 2.5%;">
            <img src=
            '<%= movie["Poster"] %>'
            >
            </div>
      </li>
    <% }) %>
</ul>

<a href="/">NEW SEARCH</a>